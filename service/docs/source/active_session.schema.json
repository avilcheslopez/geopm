{
	"$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://geopm.github.io/active_session.schema.json",
    "title": "ActiveSession",
    "type": "object",
    "properties": {
      "client_pid": {
        "type": "number"
      },
      "reference_count": {
        "type": "integer",
        "minimum": 0
      },
      "signals": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "controls": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "watch_id": {
        "type": "number"
      },
      "batch_server": {
        "type": "number"
      },
    },
    "required": ["client_pid", "signals", "controls"],
    "additionalProperties": false
}